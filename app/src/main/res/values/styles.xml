    </style>

    <style name="Profile.TextView.Title.Disciplines" parent="Profile.TextView.Header">
        <item name="android:text">@string/profile_disciplines_title</item>
    </style>

    <style name="Profile.TextView.Options" parent="Base.TextView.Inter.Light">
        <item name="android:importantForAccessibility">no</item>
        <item name="android:textColor">@color/dark_gray</item>
        <item name="android:textSize">@dimen/text_size_fourteen</item>
    </style>

    <style name="Profile.TextView.Notification" parent="Profile.TextView.Options">
        <item name="android:text">@string/profile_notification_title</item>
        <item name="android:layout_marginBottom">@dimen/spacing_twenty_four</item>
        <item name="android:layout_marginTop">@dimen/spacing_twenty_four</item>
    </style>

    <style name="Profile.TextView.Logout" parent="Profile.TextView.Options">
        <item name="android:text">@string/profile_logout_title</item>
        <item name="android:layout_marginTop">@dimen/spacing_twenty_four</item>
    </style>

    <style name="Profile.TextView.Version" parent="Profile.TextView.Options">
        <item name="android:text">@string/profile_version_title</item>
        <item name="android:layout_marginTop">@dimen/spacing_twenty_four</item>
        <item name="android:layout_marginBottom">@dimen/spacing_twenty_four</item>
    </style>

    <style name="Profile.Switch.Notification" parent="Widget.MaterialComponents.CompoundButton.Switch">
        <item name="colorControlActivated">@color/dark_purple</item>
    </style>

    <!--  Activity/Register  -->
    <style name="Register.Button.Proceed" parent="Button.Support.Default">
        <item name="android:layout_marginStart">@dimen/spacing_thirty_two</item>
        <item name="android:layout_marginEnd">@dimen/spacing_thirty_two</item>
        <item name="android:layout_marginBottom">@dimen/spacing_sixteen</item>
        <item name="android:text">@string/register_button_proceed</item>
    </style>

