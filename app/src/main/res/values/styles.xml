<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowBackground">@color/zircon</item>
        <item name="android:textColorSecondary">@color/zircon</item>
        <item name="bottomSheetDialogTheme">@style/BottomSheet.Theme</item>
    </style>

    <style name="LightPurpleTheme" parent="AppTheme">
        <item name="colorControlNormal">@color/light_purple</item>
    </style>

    <style name="Base.TextView" parent="@android:style/TextAppearance">
        <item name="android:textColor">@color/zircon</item>
        <item name="android:textSize">@dimen/text_size_sixteen</item>
    </style>

    <style name="Base.Button" parent="Widget.MaterialComponents.Button">
        <item name="android:minHeight">0dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textAllCaps">false</item>
        <item name="cornerRadius">@dimen/spacing_eight</item>
        <item name="android:padding">@dimen/spacing_sixteen</item>
    </style>

    <!--  Font/Mulish  -->
    <style name="Base.TextView.Mulish.SemiBold" parent="Base.TextView">
        <item name="android:fontFamily">@font/mulish_semi_bold</item>
    </style>

    <style name="Base.TextView.Mulish.Bold" parent="Base.TextView">
        <item name="android:fontFamily">@font/mulish_bold</item>
    </style>

    <!--  Font/Inter  -->
    <style name="Base.TextView.Inter.Light" parent="Base.TextView">
        <item name="android:fontFamily">@font/inter_light</item>
    </style>

    <style name="Base.TextView.Inter.Medium" parent="Base.TextView">
        <item name="android:fontFamily">@font/inter_medium</item>
    </style>

    <style name="Base.TextView.Inter.Regular" parent="Base.TextView">
        <item name="android:fontFamily">@font/inter_regular</item>
    </style>

    <style name="Base.TextView.Inter.SemiBold" parent="Base.TextView">
        <item name="android:fontFamily">@font/inter_semi_bold</item>
    </style>

    <style name="Base.Button.Inter.SemiBold" parent="Base.Button">
        <item name="android:fontFamily">@font/inter_semi_bold</item>
    </style>

    <!--  BottomSheet  -->
    <style name="BottomSheet.ShapeAppearance" parent="">
        <item name="cornerSizeTopLeft">@dimen/spacing_sixteen</item>
        <item name="cornerSizeTopRight">@dimen/spacing_sixteen</item>
        <item name="cornerFamily">rounded</item>
    </style>

    <style name="BottomSheet.Style" parent="Widget.MaterialComponents.BottomSheet">
        <item name="shapeAppearanceOverlay">@style/BottomSheet.ShapeAppearance</item>
    </style>

    <style name="BottomSheet.Theme" parent="@style/ThemeOverlay.MaterialComponents.BottomSheetDialog">
        <item name="bottomSheetStyle">@style/BottomSheet.Style</item>
    </style>

    <!--  Buttons  -->
    <style name="Button.Back" parent="">
        <item name="android:layout_margin">@dimen/spacing_sixteen</item>
        <item name="android:background">?selectableItemBackgroundBorderless</item>
        <item name="android:contentDescription">@string/back</item>
        <item name="android:src">@drawable/vector_back</item>
    </style>

    <style name="Button.Primary.Default" parent="Base.Button.Inter.SemiBold">
        <item name="android:textColor">@color/zircon</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:backgroundTint">@color/light_purple</item>
    </style>

    <style name="Button.Secondary.Default" parent="Base.Button.Inter.SemiBold">
        <item name="android:textColor">@color/light_purple</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:theme">@style/Theme.MaterialComponents</item>
        <item name="android:backgroundTint">@android:color/transparent</item>
        <item name="strokeColor">@color/light_purple</item>
        <item name="strokeWidth">@dimen/spacing_one</item>
    </style>

    <style name="Button.Support.Default" parent="Base.Button.Inter.SemiBold">
        <item name="android:textColor">@color/light_purple</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:backgroundTint">@color/zircon</item>
    </style>

    <style name="Button.Support.Selected" parent="Base.Button.Inter.SemiBold">
        <item name="android:textColor">@color/zircon</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:backgroundTint">@color/light_yellow</item>
    </style>

    <!--  Images  -->
    <style name="Image.Warning" parent="">
        <item name="android:layout_gravity">end|top</item>
        <item name="android:elevation">@dimen/spacing_four</item>
        <item name="android:importantForAccessibility">no</item>
        <item name="android:src">@drawable/vector_warning</item>
    </style>

    <!--  DropDown  -->
    <style name="DropDown.Zodiac" parent="Base.TextView.Inter.Regular">
        <item name="android:textColor">@color/blue_zodiac</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:textAppearance">?attr/textAppearanceSubtitle1</item>
        <item name="android:padding">@dimen/spacing_sixteen</item>
    </style>

    <!--  Divider  -->
    <style name="Divider" parent="">
        <item name="android:minHeight">@dimen/spacing_one</item>
        <item name="android:background">@color/light_gray</item>
        <item name="android:importantForAccessibility">no</item>
    </style>

    <!--  Activity/Home  -->
    <style name="Home.TextView.Title" parent="Base.TextView.Mulish.Bold">
        <item name="android:textSize">@dimen/text_size_twenty_eight</item>
        <item name="android:textColor">@color/blue_zodiac</item>
        <item name="android:text">@string/home_dashboard</item>
        <item name="android:includeFontPadding">false</item>
    </style>

    <style name="Home.TextView.CR" parent="Base.TextView.Inter.Medium">
        <item name="android:textSize">@dimen/spacing_eight</item>
        <item name="android:minWidth">@dimen/min_dashboard_cr_width</item>
        <item name="android:textColor">@color/zircon</item>
        <item name="android:background">@drawable/shape_cr</item>
        <item name="android:includeFontPadding">false</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/spacing_four</item>
        <item name="android:text">@string/dashboard_cr</item>
    </style>

    <style name="Home.Fab" parent="Base.TextView.Inter.SemiBold">
        <item name="android:textSize">@dimen/text_size_fourteen</item>
        <item name="android:textColor">@color/zircon</item>
        <item name="android:textAllCaps">false</item>
        <item name="iconTint">@color/zircon</item>
        <item name="android:backgroundTint">@color/colorAccent</item>
        <item name="icon">@drawable/vector_add</item>
    </style>

    <!--  Activity/Welcome  -->
    <style name="Welcome.Button.Explore" parent="Base.Button.Inter.SemiBold">
        <item name="android:textSize">@dimen/text_size_fourteen</item>
        <item name="android:textColor">@color/zircon</item>
        <item name="android:gravity">center</item>
        <item name="android:text">@string/welcome_button_explore</item>
        <item name="backgroundTint">@color/light_purple</item>
        <item name="android:layout_marginTop">@dimen/spacing_twenty</item>
    </style>

    <style name="Welcome.Button.Login" parent="Base.Button.Inter.SemiBold">
        <item name="android:textSize">@dimen/text_size_fourteen</item>
        <item name="android:textColor">@color/light_purple</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:theme">@style/Theme.MaterialComponents</item>
        <item name="android:text">@string/welcome_button_login</item>
        <item name="android:backgroundTint">@color/zircon</item>
        <item name="strokeColor">@color/light_purple</item>
        <item name="strokeWidth">@dimen/spacing_one</item>
        <item name="android:layout_marginTop">@dimen/spacing_sixteen</item>
    </style>

    <!--  Activity/Profile  -->
    <style name="Profile.TextView.Name" parent="Base.TextView.Mulish.SemiBold">
        <item name="android:gravity">center</item>
        <item name="android:layout_marginStart">@dimen/spacing_sixteen</item>
        <item name="android:layout_marginEnd">@dimen/spacing_sixteen</item>
        <item name="android:layout_marginTop">@dimen/spacing_eight</item>
        <item name="android:textColor">@color/zircon</item>
        <item name="android:text">@string/profile_guest_name</item>
        <item name="android:textSize">@dimen/text_size_eighteen</item>
    </style>

    <style name="Profile.TextView.Value" parent="Base.TextView.Mulish.SemiBold">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/zircon</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginTop">@dimen/spacing_thirty_two</item>
        <item name="android:text">@string/profile_empty_value</item>
        <item name="android:textSize">@dimen/text_size_twenty_four</item>
    </style>

    <style name="Profile.TextView.Header" parent="Base.TextView.Inter.Light">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/light_gray</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:layout_marginTop">@dimen/spacing_four</item>
        <item name="android:text">@string/profile_empty_value</item>
        <item name="android:textSize">@dimen/text_size_twelve</item>
    </style>

    <style name="Profile.TextView.Title.CR" parent="Profile.TextView.Header">
        <item name="android:text">@string/profile_cr_title</item>
    </style>

    <style name="Profile.TextView.Title.Semester" parent="Profile.TextView.Header">
